,zone_flu
,jurisdiction_flu
,key_flu
,definition_flu
,min_du_flu
,max_du_flu
,min_far_flu
,max_far_flu
);


create table run_104_20180406_2014.beccah_snoco_dense_prcl
(
psrc_id integer not null
,jurisdiction text
,zone text
,key_flu text
,parcel_sqft integer not null
,plan_type_id integer not null
,far float not null
,units_per_acre float not null
,primary key (psrc_id)
);

