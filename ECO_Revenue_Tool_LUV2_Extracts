inner join luv2_final_outputs_2030.jobs
on luv2_final_outputs_2030.parcels.parcel_id = luv2_final_outputs_2030.jobs.parcel_id
WHERE luv2_final_outputs_2030.jobs.sector_id in (7)
GROUP BY
city_id;


