## 2014

create table data_request_eco_revenue_tool_bazooka.res_ptba_2014
SELECT
data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.PTBA_NAME
,count(v2050_rgs_bazooka_2014.households.household_id) as households
,sum(v2050_rgs_bazooka_2014.households.persons) as persons
FROM data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st
inner join v2050_rgs_bazooka_2014.households
on data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.parcel_id = v2050_rgs_bazooka_2014.households.parcel_id
GROUP BY
PTBA_NAME;

create table data_request_eco_revenue_tool_bazooka.res_st_2014
SELECT
data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.ST_DIST
,count(v2050_rgs_bazooka_2014.households.household_id) as households
,sum(v2050_rgs_bazooka_2014.households.persons) as persons
FROM data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st
inner join v2050_rgs_bazooka_2014.households
on data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.parcel_id = v2050_rgs_bazooka_2014.households.parcel_id
GROUP BY
ST_DIST;

create table data_request_eco_revenue_tool_bazooka.res_city_2014
SELECT
v2050_rgs_bazooka_2014.parcels.city_id
,count(v2050_rgs_bazooka_2014.households.household_id) as households
,sum(v2050_rgs_bazooka_2014.households.persons) as persons
FROM v2050_rgs_bazooka_2014.parcels
inner join v2050_rgs_bazooka_2014.households
on v2050_rgs_bazooka_2014.parcels.parcel_id = v2050_rgs_bazooka_2014.households.parcel_id
GROUP BY
city_id;

create table data_request_eco_revenue_tool_bazooka.emp_ptba_2014
SELECT
data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.PTBA_NAME
,count(v2050_rgs_bazooka_2014.jobs.job_id) as jobs
FROM data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st
inner join v2050_rgs_bazooka_2014.jobs
on data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.parcel_id = v2050_rgs_bazooka_2014.jobs.parcel_id
GROUP BY
PTBA_NAME;

create table data_request_eco_revenue_tool_bazooka.emp_st_2014
SELECT
data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.ST_DIST
,count(v2050_rgs_bazooka_2014.jobs.job_id) as jobs
FROM data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st
inner join v2050_rgs_bazooka_2014.jobs
on data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.parcel_id = v2050_rgs_bazooka_2014.jobs.parcel_id
GROUP BY
ST_DIST;

create table data_request_eco_revenue_tool_bazooka.emp_city_2014
SELECT
v2050_rgs_bazooka_2014.parcels.city_id
,count(v2050_rgs_bazooka_2014.jobs.job_id) as jobs
FROM v2050_rgs_bazooka_2014.parcels
inner join v2050_rgs_bazooka_2014.jobs
on v2050_rgs_bazooka_2014.parcels.parcel_id = v2050_rgs_bazooka_2014.jobs.parcel_id
GROUP BY
city_id;

create table data_request_eco_revenue_tool_bazooka.retemp_ptba_2014
SELECT
data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.PTBA_NAME
,count(v2050_rgs_bazooka_2014.jobs.job_id) as jobs
FROM data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st
inner join v2050_rgs_bazooka_2014.jobs
on data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.parcel_id = v2050_rgs_bazooka_2014.jobs.parcel_id
WHERE v2050_rgs_bazooka_2014.jobs.sector_id in (5)
GROUP BY
PTBA_NAME;

create table data_request_eco_revenue_tool_bazooka.retemp_st_2014
SELECT
data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.ST_DIST
,count(v2050_rgs_bazooka_2014.jobs.job_id) as jobs
FROM data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st
inner join v2050_rgs_bazooka_2014.jobs
on data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.parcel_id = v2050_rgs_bazooka_2014.jobs.parcel_id
WHERE v2050_rgs_bazooka_2014.jobs.sector_id in (5)
GROUP BY
ST_DIST;

create table data_request_eco_revenue_tool_bazooka.retemp_city_2014
SELECT
v2050_rgs_bazooka_2014.parcels.city_id
,count(v2050_rgs_bazooka_2014.jobs.job_id) as jobs
FROM v2050_rgs_bazooka_2014.parcels
inner join v2050_rgs_bazooka_2014.jobs
on v2050_rgs_bazooka_2014.parcels.parcel_id = v2050_rgs_bazooka_2014.jobs.parcel_id
WHERE v2050_rgs_bazooka_2014.jobs.sector_id in (5)
GROUP BY
city_id;


