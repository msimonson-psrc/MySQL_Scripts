## Splicing Tables Together into summary tables

create table data_request_eco_revenue_tool_bazooka.city_alldata
SELECT
a.city_id
#
,a.households as hh_14
,b.households as hh_20
,c.households as hh_25
,d.households as hh_30
,e.households as hh_35
,f.households as hh_40
,g.households as hh_45
,h.households as hh_50
#
,a.persons as hhpop_14
,b.persons as hhpop_20
,c.persons as hhpop_25
,d.persons as hhpop_30
,e.persons as hhpop_35
,f.persons as hhpop_40
,g.persons as hhpop_45
,h.persons as hhpop_50
#
,i.jobs as jobs_14
,j.jobs as jobs_20
,k.jobs as jobs_25
,l.jobs as jobs_30
,m.jobs as jobs_35
,n.jobs as jobs_40
,o.jobs as jobs_45
,p.jobs as jobs_50
#
,q.jobs as retjobs_14
,r.jobs as retjobs_20
,s.jobs as retjobs_25
,t.jobs as retjobs_30
,u.jobs as retjobs_35
,v.jobs as retjobs_40
,w.jobs as retjobs_45
,x.jobs as retjobs_50
#
FROM data_request_eco_revenue_tool_bazooka.res_city_2014 a
#
Inner Join data_request_eco_revenue_tool_bazooka.res_city_2020 b on a.city_id = b.city_id
Inner Join data_request_eco_revenue_tool_bazooka.res_city_2025 c on a.city_id = c.city_id
Inner Join data_request_eco_revenue_tool_bazooka.res_city_2030 d on a.city_id = d.city_id
Inner Join data_request_eco_revenue_tool_bazooka.res_city_2035 e on a.city_id = e.city_id
Inner Join data_request_eco_revenue_tool_bazooka.res_city_2040 f on a.city_id = f.city_id
Inner Join data_request_eco_revenue_tool_bazooka.res_city_2045 g on a.city_id = g.city_id
Inner Join data_request_eco_revenue_tool_bazooka.res_city_2050 h on a.city_id = h.city_id
#
Inner Join data_request_eco_revenue_tool_bazooka.emp_city_2014 i on a.city_id = i.city_id
Inner Join data_request_eco_revenue_tool_bazooka.emp_city_2020 j on a.city_id = j.city_id
Inner Join data_request_eco_revenue_tool_bazooka.emp_city_2025 k on a.city_id = k.city_id
Inner Join data_request_eco_revenue_tool_bazooka.emp_city_2030 l on a.city_id = l.city_id
Inner Join data_request_eco_revenue_tool_bazooka.emp_city_2035 m on a.city_id = m.city_id
Inner Join data_request_eco_revenue_tool_bazooka.emp_city_2040 n on a.city_id = n.city_id
Inner Join data_request_eco_revenue_tool_bazooka.emp_city_2045 o on a.city_id = o.city_id
Inner Join data_request_eco_revenue_tool_bazooka.emp_city_2050 p on a.city_id = p.city_id
#
Inner Join data_request_eco_revenue_tool_bazooka.retemp_city_2014 q on a.city_id = q.city_id
Inner Join data_request_eco_revenue_tool_bazooka.retemp_city_2020 r on a.city_id = r.city_id
Inner Join data_request_eco_revenue_tool_bazooka.retemp_city_2025 s on a.city_id = s.city_id
Inner Join data_request_eco_revenue_tool_bazooka.retemp_city_2030 t on a.city_id = t.city_id
Inner Join data_request_eco_revenue_tool_bazooka.retemp_city_2035 u on a.city_id = u.city_id
Inner Join data_request_eco_revenue_tool_bazooka.retemp_city_2040 v on a.city_id = v.city_id
Inner Join data_request_eco_revenue_tool_bazooka.retemp_city_2045 w on a.city_id = w.city_id
Inner Join data_request_eco_revenue_tool_bazooka.retemp_city_2050 x on a.city_id = x.city_id
;

create table data_request_eco_revenue_tool_bazooka.ptba_alldata
SELECT
a.ptba_name
#
,a.households as hh_14
,b.households as hh_20
,c.households as hh_25
,d.households as hh_30
,e.households as hh_35
,f.households as hh_40
,g.households as hh_45
,h.households as hh_50
#
,a.persons as hhpop_14
,b.persons as hhpop_20
,c.persons as hhpop_25
,d.persons as hhpop_30
,e.persons as hhpop_35
,f.persons as hhpop_40
,g.persons as hhpop_45
,h.persons as hhpop_50
#
,i.jobs as jobs_14
,j.jobs as jobs_20
,k.jobs as jobs_25
,l.jobs as jobs_30
,m.jobs as jobs_35
,n.jobs as jobs_40
,o.jobs as jobs_45
,p.jobs as jobs_50
#
,q.jobs as retjobs_14
,r.jobs as retjobs_20
,s.jobs as retjobs_25
,t.jobs as retjobs_30
,u.jobs as retjobs_35
,v.jobs as retjobs_40
,w.jobs as retjobs_45
,x.jobs as retjobs_50
#
FROM data_request_eco_revenue_tool_bazooka.res_ptba_2014 a
#
Inner Join data_request_eco_revenue_tool_bazooka.res_ptba_2020 b on a.ptba_name = b.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.res_ptba_2025 c on a.ptba_name = c.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.res_ptba_2030 d on a.ptba_name = d.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.res_ptba_2035 e on a.ptba_name = e.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.res_ptba_2040 f on a.ptba_name = f.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.res_ptba_2045 g on a.ptba_name = g.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.res_ptba_2050 h on a.ptba_name = h.ptba_name
#
Inner Join data_request_eco_revenue_tool_bazooka.emp_ptba_2014 i on a.ptba_name = i.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.emp_ptba_2020 j on a.ptba_name = j.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.emp_ptba_2025 k on a.ptba_name = k.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.emp_ptba_2030 l on a.ptba_name = l.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.emp_ptba_2035 m on a.ptba_name = m.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.emp_ptba_2040 n on a.ptba_name = n.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.emp_ptba_2045 o on a.ptba_name = o.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.emp_ptba_2050 p on a.ptba_name = p.ptba_name
#
Inner Join data_request_eco_revenue_tool_bazooka.retemp_ptba_2014 q on a.ptba_name = q.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.retemp_ptba_2020 r on a.ptba_name = r.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.retemp_ptba_2025 s on a.ptba_name = s.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.retemp_ptba_2030 t on a.ptba_name = t.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.retemp_ptba_2035 u on a.ptba_name = u.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.retemp_ptba_2040 v on a.ptba_name = v.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.retemp_ptba_2045 w on a.ptba_name = w.ptba_name
Inner Join data_request_eco_revenue_tool_bazooka.retemp_ptba_2050 x on a.ptba_name = x.ptba_name
;

create table data_request_eco_revenue_tool_bazooka.st_alldata
SELECT
a.st_dist
#
,a.households as hh_14
,b.households as hh_20
,c.households as hh_25
,d.households as hh_30
,e.households as hh_35
,f.households as hh_40
,g.households as hh_45
,h.households as hh_50
#
,a.persons as hhpop_14
,b.persons as hhpop_20
,c.persons as hhpop_25
,d.persons as hhpop_30
,e.persons as hhpop_35
,f.persons as hhpop_40
,g.persons as hhpop_45
,h.persons as hhpop_50
#
,i.jobs as jobs_14
,j.jobs as jobs_20
,k.jobs as jobs_25
,l.jobs as jobs_30
,m.jobs as jobs_35
,n.jobs as jobs_40
,o.jobs as jobs_45
,p.jobs as jobs_50
#
,q.jobs as retjobs_14
,r.jobs as retjobs_20
,s.jobs as retjobs_25
,t.jobs as retjobs_30
,u.jobs as retjobs_35
,v.jobs as retjobs_40
,w.jobs as retjobs_45
,x.jobs as retjobs_50
#
FROM data_request_eco_revenue_tool_bazooka.res_st_2014 a
#
Inner Join data_request_eco_revenue_tool_bazooka.res_st_2020 b on a.st_dist = b.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.res_st_2025 c on a.st_dist = c.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.res_st_2030 d on a.st_dist = d.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.res_st_2035 e on a.st_dist = e.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.res_st_2040 f on a.st_dist = f.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.res_st_2045 g on a.st_dist = g.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.res_st_2050 h on a.st_dist = h.st_dist
#
Inner Join data_request_eco_revenue_tool_bazooka.emp_st_2014 i on a.st_dist = i.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.emp_st_2020 j on a.st_dist = j.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.emp_st_2025 k on a.st_dist = k.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.emp_st_2030 l on a.st_dist = l.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.emp_st_2035 m on a.st_dist = m.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.emp_st_2040 n on a.st_dist = n.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.emp_st_2045 o on a.st_dist = o.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.emp_st_2050 p on a.st_dist = p.st_dist
#
Inner Join data_request_eco_revenue_tool_bazooka.retemp_st_2014 q on a.st_dist = q.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.retemp_st_2020 r on a.st_dist = r.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.retemp_st_2025 s on a.st_dist = s.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.retemp_st_2030 t on a.st_dist = t.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.retemp_st_2035 u on a.st_dist = u.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.retemp_st_2040 v on a.st_dist = v.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.retemp_st_2045 w on a.st_dist = w.st_dist
Inner Join data_request_eco_revenue_tool_bazooka.retemp_st_2050 x on a.st_dist = x.st_dist
;

