alter table v2050_rgs_bazooka_2014.households add column parcel_id integer;
update v2050_rgs_bazooka_2014.households set parcel_id = 0;
alter table v2050_rgs_bazooka_2014.households add index(building_id);
update v2050_rgs_bazooka_2014.households inner join v2050_rgs_bazooka_2014.buildings
on v2050_rgs_bazooka_2014.households.building_id = v2050_rgs_bazooka_2014.buildings.building_id
set v2050_rgs_bazooka_2014.households.parcel_id = v2050_rgs_bazooka_2014.buildings.parcel_id;
alter table v2050_rgs_bazooka_2014.households add index(parcel_id);

