inner join v2050_rgs_bazooka_2040.households
on data_request_eco_revenue_tool_bazooka.bazooka_parcels_ptba_st.parcel_id = v2050_rgs_bazooka_2040.households.parcel_id
GROUP BY
ST_DIST;

create table data_request_eco_revenue_tool_bazooka.res_city_2040
SELECT
v2050_rgs_bazooka_2040.parcels.city_id
,count(v2050_rgs_bazooka_2040.households.household_id) as households
,sum(v2050_rgs_bazooka_2040.households.persons) as persons
FROM v2050_rgs_bazooka_2040.parcels
inner join v2050_rgs_bazooka_2040.households
on v2050_rgs_bazooka_2040.parcels.parcel_id = v2050_rgs_bazooka_2040.households.parcel_id
